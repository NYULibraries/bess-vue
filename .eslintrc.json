{
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential"
  ],
  "globals": {
    "CONFIG": false
  },
  "rules": {
    "semi": 2,
    "no-unused-vars": ["warn"],
    "no-debugger": ["warn"]
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
